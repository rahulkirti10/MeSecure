<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    tools:context=".contacts">

    <TextView
        android:id="@+id/partner"
        android:layout_width="323dp"
        android:layout_height="34dp"
        android:gravity="center_horizontal"
        android:text="Search Your Partner"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.862" />

    <EditText
        android:id="@+id/search"
        android:layout_width="280dp"
        android:layout_height="55dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:hint="Number"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/searchButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/searchButton"
        android:layout_width="280dp"
        android:layout_height="58dp"
        android:layout_marginBottom="284dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:drawable/ic_search_category_default" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="335dp"
        android:layout_height="46dp"
        android:gravity="center_horizontal"
        android:text="Type Number of Your Receiver"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.154" />

    <ProgressBar
        android:id="@+id/progressbar"
        android:theme="@style/progress"
        style="?android:attr/progressBarStyle"
        android:layout_width="85dp"
        android:layout_height="82dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.481" />

    <TextView
        android:id="@+id/aftersearch"
        android:layout_width="345dp"
        android:layout_height="528dp"
        android:background="#F3F2F2"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.373" />

    <TextView
        android:id="@+id/viewname"
        android:layout_width="217dp"
        android:layout_height="62dp"
        android:textSize="20sp"
        android:textAlignment="center"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/aftersearch"
        app:layout_constraintEnd_toEndOf="@+id/aftersearch"
        app:layout_constraintStart_toStartOf="@+id/aftersearch"
        app:layout_constraintTop_toTopOf="@+id/aftersearch"
        app:layout_constraintVertical_bias="0.454"
        android:gravity="center_horizontal" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="56dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toBottomOf="@+id/aftersearch"
        app:layout_constraintEnd_toEndOf="@+id/aftersearch"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/aftersearch"
        app:layout_constraintTop_toTopOf="@+id/aftersearch"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/abc_vector_test" />

    <Button
        android:id="@+id/add"
        android:layout_width="106dp"
        android:layout_height="41dp"
        android:background="#3b83dc"
        android:text="Add"
        android:textColor="#ffffff"
        app:layout_constraintBottom_toBottomOf="@+id/aftersearch"
        app:layout_constraintEnd_toEndOf="@+id/aftersearch"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/aftersearch"
        app:layout_constraintTop_toTopOf="@+id/aftersearch"
        app:layout_constraintVertical_bias="0.624" />

    <ImageView
        android:id="@+id/viewImage"
        android:layout_width="159dp"
        android:layout_height="155dp"
        android:background="@drawable/round"
        app:layout_constraintBottom_toBottomOf="@+id/aftersearch"
        app:layout_constraintEnd_toEndOf="@+id/aftersearch"
        app:layout_constraintStart_toStartOf="@+id/aftersearch"
        app:layout_constraintTop_toTopOf="@+id/aftersearch"
        app:layout_constraintVertical_bias="0.12"
        />

    <ProgressBar
        android:id="@+id/barImage"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/viewImage"
        app:layout_constraintEnd_toEndOf="@+id/viewImage"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/viewImage"
        app:layout_constraintTop_toBottomOf="@+id/partner"
        app:layout_constraintVertical_bias="0.561" />


</androidx.constraintlayout.widget.ConstraintLayout>